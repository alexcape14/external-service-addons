<?xml version="1.0" encoding="utf-8"?>
<templates>
    <div t-name="ncf_autocomplete.dropdown" class="o_ncf_autocomplete_dropdown dropdown-menu show" role="menu">
        <t t-foreach="suggestions" t-as="info">
            <a role="menuitem" href="#"
                t-attf-class="dropdown-item o_ncf_autocomplete_suggestion clearfix#{info_index == 0 and ' active' or ''}"
                t-att-data-index="info_index">
                <div class="o_ncf_autocomplete_info">
                    <strong><t t-esc="info['label'] or '&#160;'"/></strong>
                    <div><t t-esc="info['description']"/></div>
                </div>
            </a>
        </t>
    </div>
</templates>
